<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dan.kaftan.mathgame">

    <application
        android:allowBackup="true"
        android:icon="@drawable/multiplication_table_icon"
        android:label="multiplication table quiz"
        android:roundIcon="@drawable/multiplication_table_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.dan.kaftan.mathgame.MainActivity" />
        <activity android:name="com.dan.kaftan.mathgame.Game" />
        <activity android:name="com.dan.kaftan.mathgame.welcome_screen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.dan.kaftan.mathgame.EndGame" />
        <activity android:name="com.dan.kaftan.mathgame.Settings"></activity>
    </application>

</manifest>